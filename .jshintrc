{
    "curly": true,
    "freeze": true,
    "futurehostile": true,
    "globalstrict": true,
    "latedef": "nofunc",
    "maxparams": 4,
    "maxdepth": 3,
    "maxstatements": 30,
    "maxcomplexity": 10,
    "noarg": true,
    "nocomma": true,
    "node": true,
    "nonbsp": true,
    "nonew": true,
    "singleGroups": true,
    "undef": true,
    "unused": true,
    "-W097": true
}

